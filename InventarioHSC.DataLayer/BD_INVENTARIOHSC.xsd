                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT
		USU.Nombre Usuario,
		UBI.Descripcion Ubicacion,
		REG.Nombre Region,
		TEQ.Descripcion TipoActivo,
		ART.NoSerie,
		ART.CodigoCastor,
		ART.Observacion1,
		ART.Observacion2,
		ART.Observacion3,
		'MODELO:' + LTRIM(RTRIM(ART.Modelo)) +
		', PROCESADOR: ' + ART.Procesador +
		', RAM: ' + ART.RAM +
		', DISCO DURO: ' + ART.DiscoDuro +
		', SISTEMA OPERATIVO: ' + SO.Descripcion +
		', MARCA: ' + MAR.Descripcion +
		', PROVEEDOR: ' + PRO.Descripcion AS Observacion,
		ART.Responsiva,
		CASE
			WHEN ISNULL(JCB.NoSerie, '') = '' THEN
				'JCB'
			WHEN ISNULL(AC.NoSerie, '') = '' THEN
				'CORP'
			ELSE
				'HSC'
		END Tipo
FROM Articulo ART
LEFT JOIN
	ArticuloJCB JCB ON
	ART.NoSerie = JCB.NoSerie
LEFT JOIN
	ArticuloCorporativo AC ON
	ART.NoSerie = AC.NoSerie
INNER JOIN
	TipoEquipo TEQ ON
	ART.idTipoEquipo = TEQ.idTipoEquipo
LEFT JOIN
	Usuario USU ON
	ART.idUsuario = USU.idUsuario
LEFT JOIN
	Ubicacion UBI ON
	ART.idUbicacion = UBI.idUbicacion
LEFT JOIN
	Region REG ON
	UBI.idRegion = REG.idRegion
LEFT JOIN
	Marca MAR ON
	ART.idMarca = MAR.idMarca
LEFT JOIN
	Proveedor PRO ON
	ART.idProveedor = PRO.idProveedor
LEFT JOIN
	SistemaOperativo SO ON
	ART.idSistema = SO.idSistema
WHERE ART.idUbicacion = @idUbicacion
ORDER BY USU.Nombre, ART.NoSerie, JCB.NoSerie, AC.NoSerie</CommandText>
