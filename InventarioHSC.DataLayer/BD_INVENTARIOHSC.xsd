            <MainSource>
              <DbSource ConnectionRef="hdbqa08.BD_INVENTARIOHSC.dbo" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill_ReporteMovimientos" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData_ReporteMovimientos" GeneratorSourceName="Fill_ReporteMovimientos" GetMethodModifier="Public" GetMethodName="GetData_ReporteMovimientos" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetData_ReporteMovimientos" UserSourceName="Fill_ReporteMovimientos">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT
		USU.Nombre Usuario,
		UBI.Descripcion Ubicacion,
		REG.Nombre Region,
		TEQ.Descripcion TipoActivo,
		ART.NoSerie,
		ART.CodigoCastor,
		ART.Observacion1,
		ART.Observacion2,
		ART.Observacion3,
		CASE
			WHEN ART.idTipoEquipo  = 1 OR ART.idTipoEquipo  = 2 OR ART.idTipoEquipo = 3 THEN
				'MODELO:' + ISNULL(LTRIM(RTRIM(ART.Modelo)), '') +
				', PROCESADOR: ' + ISNULL(ART.Procesador, '') +
				', RAM: ' + ISNULL(ART.RAM, '') +
				', DISCO DURO: ' + ISNULL(ART.DiscoDuro, '') +
				', SISTEMA OPERATIVO: ' + ISNULL(SO.Descripcion, '') +
				', MARCA: ' + ISNULL(MAR.Descripcion, '') +
				', PROVEEDOR: ' + ISNULL(PRO.Descripcion, '')
			ELSE
				'MARCA: ' + ISNULL(MAR.Descripcion, '') +
				', PROVEEDOR: ' + ISNULL(PRO.Descripcion, '')
		END AS Observacion,
		ART.Responsiva,
		CASE
			WHEN ISNULL(JCB.NoSerie, '') = '' THEN 'JCB'
			WHEN ISNULL(AC.NoSerie, '') = '' THEN 'CORP'
			WHEN ISNULL(ART.NoSerie, '') = '' THEN 'HSC'
		END AS Tipo,
		TM.Descripcion,
		CONVERT(VARCHAR(10), HM.fechaMovimiento, 103) FechaMov,
		CONVERT(VARCHAR(10), HM.fechaTransaccion, 103) FechaTran
FROM Articulo ART
LEFT JOIN
	ArticuloCorporativo AC ON
	ART.NoSerie = AC.NoSerie
LEFT JOIN
	ArticuloJCB JCB ON
	ART.NoSerie = JCB.NoSerie
INNER JOIN
	Historico_Movimiento HM ON
	HM.idItem = ART.idItem
INNER JOIN
	TipoMovimiento TM ON
	HM.idTipoMovimiento = tm.idTipoMovimiento
LEFT JOIN
	TipoEquipo TEQ ON
	ART.idTipoEquipo = TEQ.idTipoEquipo
LEFT JOIN
	Usuario USU ON
	ART.idUsuario = USU.idUsuario
LEFT JOIN
	Ubicacion UBI ON
	ART.idUbicacion = UBI.idUbicacion
AND UBI.Estatus = 1
LEFT JOIN
	Region REG ON
	UBI.idRegion = REG.idRegion
AND REG.Estatus = 1
LEFT JOIN
	Marca MAR ON
	ART.idMarca = MAR.idMarca
LEFT JOIN
	Proveedor PRO ON
	ART.idProveedor = PRO.idProveedor
LEFT JOIN
	SistemaOperativo SO ON
	ART.idSistema = SO.idSistema
WHERE ART.NoSerie = @NoSerie
ORDER BY ART.NoSerie
</CommandText>
