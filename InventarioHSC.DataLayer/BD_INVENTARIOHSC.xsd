                      <Parameter AllowDbNull="false" AutogeneratedName="NoSerie" ColumnName="NoSerie" DataSourceName="BD_INVENTARIOHSC.dbo.Articulo" DataTypeServer="varchar(50)" DbType="AnsiString" Direction="Input" ParameterName="@NoSerie" Precision="0" ProviderType="VarChar" Scale="0" Size="50" SourceColumn="NoSerie" SourceColumnNullMapping="false" SourceVersion="Current" />
